programa
{
	
	funcao inicio()
	{

	inteiro n1,n2,nn1

		escreva("digite um número: ")
			leia(n1)
		escreva("digite outro número: ")
			leia(n2)

			nn1= n1
			n1= n2
			n2= nn1
			

			escreva("número um: ",n1,"\nnúmero dois: ",n2)
	}
}

